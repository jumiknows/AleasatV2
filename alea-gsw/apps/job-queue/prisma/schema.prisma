// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider        = "prisma-client-js"
    previewFeatures = []
    output          = "../../../node_modules/@prisma/client/job-queue"
}

datasource db {
    provider = "postgresql"
    url      = env("POSTGRES_URL")
}

model Job {
    id     String  @id @unique @default(uuid()) @db.Uuid
    name   String
    type   JobType
    status String

    requestData Json
    resultData  Json?
}

enum JobType {
    GMAT
    ORBIT_PROPAGATION
}
