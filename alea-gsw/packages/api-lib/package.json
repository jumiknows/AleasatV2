{
  "name": "@aleasat/api-lib",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "swc src  --strip-leading-paths --out-dir dist",
    "build:types": "tsc",
    "test:unit": "mocha --no-warnings 'tests/unit/**/*.spec.ts'",
    "test:unit-coverage": "c8 yarn test:unit",
    "test:ci-unit": "c8 yarn test:unit --reporter mocha-junit-reporter",
    "test:types": "tsc --noEmit",
    "test:ci-copy-result-and-coverage": "shx mv test-results.xml ../../ && shx mv coverage ../../",
    "lint": "biome check",
    "lint:ci": "biome ci",
    "lint:fix": "biome check --fix"
  },
  "dependencies": {
    "@aleasat/utils": "*",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@aleasat/tsconfig": "*",
    "@aleasat/types": "*",
    "@biomejs/biome": "1.9.4",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.12",
    "@types/chai-as-promised": "^7",
    "@types/express": "^4",
    "@types/mocha": "^10",
    "@types/node": "^20.8.10",
    "@types/sinon": "^17",
    "c8": "^10.1.3",
    "chai": "^5.0.0",
    "chai-as-promised": "^7.1.1",
    "express": "^4.18.2",
    "mocha": "11.1.0",
    "mocha-junit-reporter": "^2.2.1",
    "nodemon": "^3.0.1",
    "openapi-merge-cli": "^1.3.1",
    "openapi-typescript": "^6.7.1",
    "shx": "^0.3.4",
    "sinon": "^17.0.1",
    "ts-node": "patch:ts-node@npm%3A10.9.2#~/.yarn/patches/ts-node-npm-10.9.2-3f3890b9ac.patch",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
