{
  "name": "@aleasat/pass-scheduling",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "swc src  --strip-leading-paths --out-dir dist",
    "build:types": "yarn build",
    "test:unit": "NODE_ENV=testing mocha --no-warnings 'tests/unit/**/*.spec.ts'",
    "test:unit-coverage": "c8 yarn test:unit",
    "test:ci-unit": "c8 yarn test:unit --reporter mocha-junit-reporter",
    "test:types": "tsc --noEmit",
    "test:ci-copy-result-and-coverage": "shx mv test-results.xml ../../ && shx mv coverage ../../",
    "lint": "biome check",
    "lint:ci": "biome ci",
    "lint:fix": "biome check --fix",
    "graphql-codegen": "graphql-codegen-esm --config graphql-codegen.ts && eslint --fix ./src/gql/generated/sdk.ts || exit 0",
    "dev": "nodemon --exec node --no-warnings=ExperimentalWarning --loader ts-node/esm --loader @opentelemetry/instrumentation/hook.mjs --enable-source-maps --watch . src/server.ts",
    "prisma": "dotenvx run --convention=nextjs -- prisma",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@aleasat/api-lib": "*",
    "@aleasat/command-control-api": "*",
    "@aleasat/orbit-propagation-api": "*",
    "@aleasat/pass-scheduling-api": "*",
    "@aleasat/utils": "*",
    "@dotenvx/dotenvx": "^1.20.0",
    "@opentelemetry/api": "^1.9.0",
    "@prisma/client": "^6.5.0",
    "@prisma/instrumentation": "^6.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-openapi-validator": "^4.13.8",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.5.0",
    "remove": "^0.1.5",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@aleasat/tsconfig": "*",
    "@aleasat/types": "*",
    "@biomejs/biome": "1.9.4",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^6.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.12",
    "@types/chai-as-promised": "^7",
    "@types/cors": "^2",
    "@types/eslint": "^8",
    "@types/express": "^4",
    "@types/mocha": "^10",
    "@types/node": "^20.8.10",
    "@types/sinon": "^17",
    "@types/swagger-ui-express": "^4.1.6",
    "axios": "^1.7.2",
    "c8": "^10.1.3",
    "chai": "^5.0.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "11.1.0",
    "mocha-junit-reporter": "^2.2.1",
    "nodemon": "^3.0.1",
    "shx": "^0.3.4",
    "sinon": "^17.0.1",
    "testdouble": "^3.20.1",
    "ts-node": "patch:ts-node@npm%3A10.9.2#~/.yarn/patches/ts-node-npm-10.9.2-3f3890b9ac.patch",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
