{
  "name": "@aleasat/mops-client-grpc-node",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./obc": "./dist/generated/obc.js"
  },
  "typesVersions": {
    "*": {
      "obc": [
        "dist/generated/obc.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "swc src  --strip-leading-paths --out-dir dist",
    "build:types": "tsc",
    "test:unit": "mocha --no-warnings 'tests/unit/**/*.spec.ts'",
    "test:unit-coverage": "c8 yarn test:unit",
    "test:ci-unit": "c8 yarn test:unit --reporter mocha-junit-reporter",
    "test:types": "tsc --noEmit",
    "test:ci-copy-result-and-coverage": "shx mv test-results.xml ../../ && shx mv coverage ../../",
    "lint": "biome check",
    "lint:ci": "biome ci",
    "lint:fix": "biome check --fix",
    "start": "node dist/index.js",
    "dev": "nodemon --exec node --no-warnings=ExperimentalWarning --loader ts-node/esm --loader @opentelemetry/instrumentation/hook.mjs --watch src src/index.ts",
    "gen": "grpc_tools_node_protoc  --plugin=protoc-gen-ts_proto=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=false,esModuleInterop=true,importSuffix=.js,outputPartialMethods=false -I=../../protos obc.proto"
  },
  "dependencies": {
    "@aleasat/utils": "*",
    "express": "^4.18.2",
    "jwt-decode": "^4.0.0"
  },
  "devDependencies": {
    "@aleasat/tsconfig": "*",
    "@aleasat/types": "*",
    "@biomejs/biome": "1.9.4",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.12",
    "@types/chai-as-promised": "^7",
    "@types/express": "^4",
    "@types/mocha": "^10",
    "@types/node": "^20.8.10",
    "@types/sinon": "^17",
    "c8": "^10.1.3",
    "chai": "^5.0.0",
    "chai-as-promised": "^7.1.1",
    "grpc-tools": "^1.12.4",
    "mocha": "11.1.0",
    "mocha-junit-reporter": "^2.2.1",
    "nice-grpc": "^2.1.7",
    "nodemon": "^3.0.1",
    "shx": "^0.3.4",
    "sinon": "^17.0.1",
    "ts-node": "patch:ts-node@npm%3A10.9.2#~/.yarn/patches/ts-node-npm-10.9.2-3f3890b9ac.patch",
    "ts-proto": "^1.164.0",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
