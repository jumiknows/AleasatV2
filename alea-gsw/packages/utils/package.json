{
  "name": "@aleasat/utils",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "swc src  --strip-leading-paths --out-dir dist",
    "build:types": "tsc",
    "test:unit": "mocha --no-warnings 'tests/unit/**/*.spec.ts'",
    "test:unit-coverage": "c8 yarn test:unit",
    "test:ci-unit": "c8 yarn test:unit --reporter mocha-junit-reporter",
    "test:types": "tsc --noEmit",
    "test:ci-copy-result-and-coverage": "shx mv test-results.xml ../../ && shx mv coverage ../../",
    "lint": "biome check",
    "lint:ci": "biome ci",
    "lint:fix": "biome check --fix"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.20.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-metrics": "^0.33.0",
    "@opentelemetry/auto-instrumentations-node": "^0.51.0",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.53.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.53.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.53.0",
    "@opentelemetry/instrumentation": "^0.53.0",
    "@opentelemetry/resources": "^1.26.0",
    "@opentelemetry/sdk-logs": "^0.53.0",
    "@opentelemetry/sdk-metrics": "^1.26.0",
    "@opentelemetry/sdk-node": "^0.53.0",
    "@opentelemetry/sdk-trace-base": "^1.26.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@prisma/instrumentation": "^6.5.0",
    "jose": "^5.9.4",
    "jwt-decode": "^4.0.0"
  },
  "devDependencies": {
    "@aleasat/tsconfig": "*",
    "@aleasat/types": "*",
    "@biomejs/biome": "1.9.4",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.12",
    "@types/chai-as-promised": "^7",
    "@types/express": "^4",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/mocha": "^10",
    "@types/node": "^20.8.10",
    "@types/sinon": "^17",
    "c8": "^10.1.3",
    "chai": "^5.0.0",
    "chai-as-promised": "^7.1.1",
    "express": "^4.18.2",
    "mocha": "11.1.0",
    "mocha-junit-reporter": "^2.2.1",
    "shx": "^0.3.4",
    "sinon": "^17.0.1",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
